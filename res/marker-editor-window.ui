<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.10"/>
  <object class="GtkImage" id="refresh">
    <property name="icon_name">view-refresh-symbolic</property>
    <property name="icon_size">2</property>
  </object>
  <object class="GtkImage" id="doc-open">
    <property name="icon_name">document-open-symbolic</property>
    <property name="icon_size">2</property>
  </object>
  <object class="GtkImage" id="open-menu">
    <property name="icon_name">open-menu-symbolic</property>
    <property name="icon_size">2</property>
  </object>
  <template class="MarkerEditorWindow" parent="GtkWindow">
    <property name="default_width">600</property>
    <property name="default_height">400</property>
    <child type="titlebar">
      <object class="GtkHeaderBar">
        <property name="title">Untitled.md</property>
        <property name="show_close_button">True</property>
        <child>
          <object class="GtkButton">
            <property name="receives_default">False</property>
            <property name="label">Open...</property>
          </object>
          <packing>
            <property name="pack_type">start</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton">
            <property name="receives_default">False</property>
            <property name="label">Save</property>
          </object>
          <packing>
            <property name="pack_type">start</property>
          </packing>
        </child>
        <child>
          <object class="GtkToggleButton">
            <property name="image">open-menu</property>
          </object>
          <packing>
            <property name="pack_type">end</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton">
            <property name="image">refresh</property>
          </object>
          <packing>
            <property name="pack_type">end</property>
          </packing>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkPaned">
        <child>
          <object class="GtkSourceView">
          </object>
        </child>
        <child>
          <object class="WebkitWebView">
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
